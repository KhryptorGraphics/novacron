{
  "model": "Node Reliability Predictor",
  "version": "1.0.0",
  "architecture": "DQN",
  "created": "2025-11-14",
  "status": "production-ready",

  "target_metrics": {
    "accuracy": 0.85,
    "mae": 0.05,
    "rmse": 0.08,
    "r2": 0.90
  },

  "features": {
    "input_size": 4,
    "features": [
      {
        "name": "uptime_percentage",
        "range": [0, 100],
        "weight": 0.4,
        "description": "Historical uptime percentage"
      },
      {
        "name": "failure_rate",
        "range": [0, 10],
        "weight": 0.3,
        "description": "Failures per hour"
      },
      {
        "name": "network_quality_score",
        "range": [0, 1],
        "weight": 0.2,
        "description": "Network quality metric"
      },
      {
        "name": "geographic_distance",
        "range": [0, 10000],
        "weight": 0.1,
        "description": "Distance from requester in km"
      }
    ]
  },

  "architecture_details": {
    "layers": [
      {
        "type": "Dense",
        "units": 64,
        "activation": "relu",
        "regularization": "l2(0.01)"
      },
      {
        "type": "Dropout",
        "rate": 0.2
      },
      {
        "type": "Dense",
        "units": 32,
        "activation": "relu",
        "regularization": "l2(0.01)"
      },
      {
        "type": "Dropout",
        "rate": 0.2
      },
      {
        "type": "Dense",
        "units": 16,
        "activation": "relu"
      },
      {
        "type": "Dense",
        "units": 1,
        "activation": "sigmoid"
      }
    ],
    "optimizer": "Adam",
    "learning_rate": 0.001,
    "loss": "mse"
  },

  "training_config": {
    "epochs": 100,
    "batch_size": 32,
    "validation_split": 0.15,
    "test_split": 0.15,
    "total_samples": 10000
  },

  "performance": {
    "inference_time_single": "< 1ms",
    "inference_time_batch_100": "< 10ms",
    "model_size_kb": 50,
    "memory_usage_mb": 100
  },

  "deployment": {
    "api_endpoint": "/api/ml/predict-reliability",
    "api_port": 5002,
    "docker_image": "novacron/reliability-predictor:latest",
    "kubernetes_replicas": 3
  },

  "examples": {
    "high_reliability": {
      "input": {
        "uptime": 99.9,
        "failure_rate": 0.01,
        "network_quality": 0.95,
        "distance": 50
      },
      "expected_output": 0.95
    },
    "medium_reliability": {
      "input": {
        "uptime": 85.0,
        "failure_rate": 0.5,
        "network_quality": 0.70,
        "distance": 2000
      },
      "expected_output": 0.70
    },
    "low_reliability": {
      "input": {
        "uptime": 60.0,
        "failure_rate": 5.0,
        "network_quality": 0.30,
        "distance": 8000
      },
      "expected_output": 0.35
    }
  },

  "tests": {
    "unit_tests": 20,
    "code_coverage": 0.90,
    "edge_cases_tested": true
  },

  "files": {
    "model": "backend/ml/models/reliability_predictor.py",
    "weights": "backend/ml/models/reliability_predictor.weights.h5",
    "history": "backend/ml/models/reliability_predictor_history.json",
    "api": "backend/ml/models/reliability_predictor_api.py",
    "tests": "tests/ml/test_reliability_predictor.py",
    "training_script": "backend/ml/train_reliability_model.sh"
  }
}
