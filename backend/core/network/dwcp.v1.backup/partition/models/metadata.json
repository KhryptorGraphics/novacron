{
  "model_name": "dqn_v1",
  "version": "1.0.0",
  "description": "Deep Q-Network for DWCP Task Partitioning",
  "architecture": {
    "type": "DQN",
    "state_size": 20,
    "action_size": 15,
    "hidden_layers": [128, 128, 64],
    "activation": "relu",
    "output_activation": "linear"
  },
  "training": {
    "framework": "TensorFlow 2.13+",
    "optimizer": "Adam",
    "learning_rate": 0.001,
    "gamma": 0.95,
    "epsilon_start": 1.0,
    "epsilon_min": 0.01,
    "epsilon_decay": 0.995,
    "replay_buffer_size": 10000,
    "batch_size": 32,
    "episodes": 1000,
    "update_target_frequency": 10
  },
  "features": {
    "prioritized_replay": true,
    "double_dqn": true,
    "batch_normalization": true,
    "dropout": 0.2
  },
  "input_features": [
